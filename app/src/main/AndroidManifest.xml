<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project_kbm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fti_update"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_fti_update_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project_KBM"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.CommitteeDetailActivity"
            android:exported="true" />
        <activity
            android:name=".activity.WebViewActionActivity"
            android:exported="true" />
        <activity
            android:name=".activity.NewsDetailActivity"
            android:exported="true" />
        <activity
            android:name=".activity.EventDetailActivity"
            android:theme="@style/Theme.EventDetailTheme" />
        <activity android:name=".activity.ForgetPasswordActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>